remDr$navigate("http://www.baidu.com/")
remDr$getCurrentUrl()
webElem <- remDr$findElement(using = "partial link text", '//LI[@class=" ant-pagination-next"]/A[1]')
sapply(webElem, function(x){x$highlightElement(4)})
webElem$highlightElement(10)
webElem$sendKeysToElement(list("R Cran"))
webElem[[1]]$getElementLocation()[c("x","y")]

remDr$navigate("http://www.google.com/ncr")
webElem <- remDr$findElement(using = "css", "[name = 'q']")
webElem$sendKeysToElement(list("R Cran"))

remDr$mouseMoveToLocation(webElement = webElem[[1]]) # move mouse to the element we selected
remDr$click(2) # 2 indicates click the right mouse button
remDr$sendKeysToActiveElement(
  list(key = 'down_arrow', key = 'down_arrow', key = 'enter')
)

remDr$goBack()

rD <- rsDriver(verbose = FALSE)
